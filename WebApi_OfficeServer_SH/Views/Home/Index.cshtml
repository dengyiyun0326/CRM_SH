@model List<WebApi_OfficeServer_SH.Models.CaseQueue>
@{
    int j1, j6;
    int m1 = 0, m6 = 0;
    List<WebApi_OfficeServer_SH.Models.CaseQueue> Local_List = Model.Where(q => q.BelongQueue.Contains("APAC")).ToList();
    List<WebApi_OfficeServer_SH.Models.CaseQueue> Global_List = Model.Where(q => !q.BelongQueue.Contains("APAC")).ToList();
}

<h3 class="text-center" style="color:purple;font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif">APAC Queue</h3>

@for (var i = 0; ; i++)
{
    if (!Local_List.Any())
    {
        <h4 class="text-center" style="color:darkgrey">No case in this queue now. Have a relax :)</h4>
        break;
    }

    <div class="row">
        @for (j1 = 0; j1 < 4; j1++)
        {
            <div class="col-md-3">
                <hr />
                <!--Bypass and Assgined-->
                @if (Local_List[i + j1 + m1].IfAssign == "true")
                {
                    <h4 class="text-center" style="color:darkblue;">@Html.DisplayFor(item => Local_List[i + j1 + m1].CaseId)</h4>
                    <h4 style="color:darkblue">SLA: @Html.DisplayFor(item => Local_List[i + j1 + m1].EndTimeSla)</h4>
                    <p style="color:darkblue">Title: @Html.DisplayFor(item => Local_List[i + j1 + m1].CustomerTitle)</p>
                    <b style="color:darkblue">Country: @Html.DisplayFor(item => Local_List[i + j1 + m1].CallingCountry)</b>
                    <p style="color:darkblue">Queue: @Html.DisplayFor(item => Local_List[i + j1 + m1].BelongQueue)</p>
                    <h3 style="color:darkblue">Assigned</h3>
                }
                else if (Local_List[i + j1 + m1].IfBypass == "true")
                {
                    <h4 class="text-center" style="color:grey;">@Html.DisplayFor(item => Local_List[i + j1 + m1].CaseId)</h4>
                    <h4 style="color:grey">SLA: @Html.DisplayFor(item => Local_List[i + j1 + m1].EndTimeSla)</h4>
                    <p style="color:grey">Title: @Html.DisplayFor(item => Local_List[i + j1 + m1].CustomerTitle)</p>
                    <b style="color:grey">Country: @Html.DisplayFor(item => Local_List[i + j1 + m1].CallingCountry)</b>
                    <p style="color:grey">Queue: @Html.DisplayFor(item => Local_List[i + j1 + m1].BelongQueue)</p>
                    <h3 style="color:slategrey">Bypassed</h3>
                }
                else
                {
                    <h4 class="text-center" style="color:darkblue;">@Html.DisplayFor(item => Local_List[i + j1 + m1].CaseId)</h4>
                    <h4 style="color:red">SLA: @Html.DisplayFor(item => Local_List[i + j1 + m1].EndTimeSla)</h4>
                    <p class="lead">Title: @Html.DisplayFor(item => Local_List[i + j1 + m1].CustomerTitle)</p>
                    <b>Country: @Html.DisplayFor(item => Local_List[i + j1 + m1].CallingCountry)</b>
                    <p>Queue: @Html.DisplayFor(item => Local_List[i + j1 + m1].BelongQueue)</p>
                    @Html.ActionLink("Assign", "Assigned", new { id = Local_List[i + j1 + m1].CaseId }, new { @class = "btn btn-success", @style = "margin-right:5px" })
                    @Html.ActionLink("Bypass", "Bypassed", new { id = Local_List[i + j1 + m1].CaseId }, new { @class = "btn btn-warning" })
                }
            </div>

            if (Local_List[i + j1 + m1].Equals(Local_List.LastOrDefault()))
            {
                break;
            }
        }
    </div>
    if (j1 == 4)
    {
        if (Local_List[i + j1 + m1 - 1].Equals(Local_List.LastOrDefault()))
        {
            break;
        }
    }
    else
    {
        if (Local_List[i + j1 + m1].Equals(Local_List.LastOrDefault()))
        {
            break;
        }
    }

    m1 = m1 + 3;
}



