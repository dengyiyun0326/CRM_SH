@model List<WebApi_OfficeServer_SH.Models.CaseQueue>
@{
    int j1,j2,j3,j4,j5,j6;
    int m1 = 0,m2 = 0,m3 = 0,m4 = 0,m5 = 0,m6 = 0;
    List<WebApi_OfficeServer_SH.Models.CaseQueue> APAC_Core_List = Model.Where(q => q.BelongQueue == "APAC Office Core Apps").ToList();
    List<WebApi_OfficeServer_SH.Models.CaseQueue> APAC_Shared_List = Model.Where(q => q.BelongQueue == "APAC Office Shared Services").ToList();
    List<WebApi_OfficeServer_SH.Models.CaseQueue> APAC_Activation_List = Model.Where(q => q.BelongQueue == "APAC Office Client Activation and Development").ToList();
    List<WebApi_OfficeServer_SH.Models.CaseQueue> APAC_Dev_List = Model.Where(q => q.BelongQueue == "APAC Office Development (VSOD)").ToList();
    List<WebApi_OfficeServer_SH.Models.CaseQueue> APAC_EBA_List = Model.Where(q => q.BelongQueue == "APAC EBA").ToList();
    List<WebApi_OfficeServer_SH.Models.CaseQueue> Global_List = Model.Where(q => !q.BelongQueue.Contains("APAC")).ToList();
}

<h4 class="text-center" style="color:purple;font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif">APAC Office Core Apps</h4>

@for (var i = 0; ; i++)
{
    if (!APAC_Core_List.Any())
    {
        <h4 class="text-center" style="color:darkgrey">No case in this queue now. Have a relax :)</h4>
        break;
    }

    <div class="row">
        @for (j1 = 0; j1 < 4; j1++)
        {
            <div class="col-md-3">
                <hr />
                <!--Bypass and Assgined-->
                @if (APAC_Core_List[i + j1 + m1].IfAssign == "true")
                {
                    <h4 class="text-center" style="color:darkblue;">@Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].CaseId)</h4>
                    <h4 style="color:darkblue">SLA: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].EndTimeSla)</h4>
                    <p style="color:darkblue">Title: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].CustomerTitle)</p>
                    <b style="color:darkblue">Country: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].CallingCountry)</b>
                    <p style="color:darkblue">Queue: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].BelongQueue)</p>
                    <h3 style="color:darkblue">Assigned</h3>
                }
                else if (APAC_Core_List[i + j1 + m1].IfBypass == "true")
                {
                    <h4 class="text-center" style="color:grey;">@Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].CaseId)</h4>
                    <h4 style="color:grey">SLA: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].EndTimeSla)</h4>
                    <p style="color:grey">Title: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].CustomerTitle)</p>
                    <b style="color:grey">Country: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].CallingCountry)</b>
                    <p style="color:grey">Queue: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].BelongQueue)</p>
                    <h3 style="color:slategrey">Bypassed</h3>
                }
                else
                {
                    <h4 class="text-center" style="color:darkblue;">@Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].CaseId)</h4>
                    <h4 style="color:red">SLA: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].EndTimeSla)</h4>
                    <p class="lead">Title: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].CustomerTitle)</p>
                    <b>Country: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].CallingCountry)</b>
                    <p>Queue: @Html.DisplayFor(item => APAC_Core_List[i + j1 + m1].BelongQueue)</p>
                    @Html.ActionLink("Assign", "Assigned", new { id = APAC_Core_List[i + j1 + m1].CaseId }, new { @class = "btn btn-success", @style = "margin-right:5px" })
                    @Html.ActionLink("Bypass", "Bypassed", new { id = APAC_Core_List[i + j1 + m1].CaseId }, new { @class = "btn btn-warning" })
                }
            </div>

            if (APAC_Core_List[i + j1 + m1].Equals(APAC_Core_List.LastOrDefault()))
            {
                break;
            }
        }
    </div>

    if (APAC_Core_List[i + j1 + m1].Equals(APAC_Core_List.LastOrDefault()))
    {
        break;
    }

    m1 = m1 + 3;
}


